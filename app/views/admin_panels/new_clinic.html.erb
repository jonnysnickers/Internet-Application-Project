<% provide(:title, 'New clinic') %>
<h1>Create new clinic</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user, url: "/create_clinic") do |f| %>
      <%= render 'shared/error_messages' %>
    
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit "Create clinic", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>